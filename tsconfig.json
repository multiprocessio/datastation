{
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noEmit": true,
    "jsx": "react",
    "allowJs": true,
    "target": "esnext",
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "typeRoots": ["./node_modules/@types", "./type-overrides"]
  },
  "exclude": ["node_modules", "*.js", "build"]
}
