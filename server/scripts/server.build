setenv UI_CONFIG_OVERRIDES "window.DS_CONFIG_MODE = 'server';"
yarn build-ui

yarn esbuild server/runner.ts --sourcemap --platform=node --target=node10.4 --outfile=build/server_runner.js
yarn esbuild server/exporter.ts --sourcemap --platform=node --target=node10.4 --outfile=build/exporter.js
yarn esbuild server/index.ts --sourcemap --platform=node --target=node10.4 --outfile=build/server.js
cp server/migrations

setenv_default SERVER_CONFIG_OVERRIDES ""
prepend {SERVER_CONFIG_OVERRIDES} build/server.js