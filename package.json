{
  "name": "datastation",
  "productName": "DataStation Community Edition",
  "license": "Apache-2.0",
  "description": "The Data IDE for Developers",
  "author": "DataStation Authors",
  "main": "build/desktop.js",
  "scripts": {
    "typecheck": "yarn tsc",
    "build-desktop": "python3 ./scripts/build.py ./desktop/scripts/desktop.build",
    "build-ui": "python3 ./scripts/build.py ./ui/scripts/ui.build",
    "build-server": "python3 ./scripts/build.py ./server/scripts/server.build",
    "start-server": "./server/scripts/watch_and_serve.sh",
    "start-desktop": "yarn build-ui && yarn build-desktop && yarn electron --trace-warning --unhandled-rejection=strict build/desktop.js",
    "start-ui": "./ui/scripts/watch_and_serve.sh",
    "release-desktop": "python3 ./scripts/build.py ./desktop/scripts/release.build",
    "format": "yarn prettier --write \"**/*.js\" \"**/*.ts\" \"**/*.tsx\" \"**/*.css\" \"**/*.json\"",
    "test-licenses": "yarn license-checker --production --onlyAllow 'AFLv2.1;BSD;MIT;Apache-2.0;ISC;BSD-3-Clause;MIT*;BSD-2-Clause;Unlicense'",
    "test": "yarn jest --passWithNoTests",
    "test-coverage": "yarn jest --passWithNoTests --coverage",
    "e2e-test": "node --unhandled-rejections=throw ./e2e/index.js"
  },
  "dependencies": {
    "@hapi/hapi": "^20.1.5",
    "@hapi/inert": "^6.0.4",
    "ace-builds": "^1.4.12",
    "alasql": "^1.7.3",
    "chart.js": "^3.3.2",
    "date-fns": "^2.22.1",
    "jsesc": "^3.0.2",
    "json-stringify-safe": "^5.0.1",
    "lodash.debounce": "^4.0.8",
    "mssql": "^7.2.1",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "pako": "^2.0.3",
    "papaparse": "^5.3.1",
    "parquetjs-lite": "^0.8.7",
    "pg": "^8.6.0",
    "preview": "https://github.com/multiprocessio/preview",
    "prismjs": "^1.24.1",
    "react": "^17.0.2",
    "react-ace": "^9.4.1",
    "react-dom": "^17.0.2",
    "react-syntax-highlighter": "^15.4.4",
    "shape": "https://github.com/multiprocessio/shape",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2",
    "ssh2-promise": "^1.0.0",
    "ssh2-sftp-client": "^7.0.0",
    "tmp-promise": "^3.0.2",
    "uuid": "^8.3.2",
    "xlsx": "^0.17.0"
  },
  "optionalDependencies": {
    "oracledb": "^5.2.0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/hapi__hapi": "^20.0.9",
    "@types/hapi__inert": "^5.2.3",
    "@types/jsesc": "^3.0.1",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/lodash.debounce": "^4.0.6",
    "@types/mssql": "^7.1.3",
    "@types/node-fetch": "^2.5.10",
    "@types/oracledb": "^5.2.1",
    "@types/pako": "^1.0.1",
    "@types/papaparse": "^5.2.5",
    "@types/pg": "^8.6.0",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/refractor": "^3.0.2",
    "@types/ssh2-sftp-client": "^5.3.2",
    "@types/uuid": "^8.3.0",
    "electron": "^13.1.2",
    "electron-packager": "^15.2.0",
    "esbuild": "^0.12.7",
    "esbuild-jest": "^0.5.0",
    "jest": "^27.0.6",
    "license-checker": "^25.0.1",
    "prettier": "^2.3.0",
    "prettier-plugin-organize-imports": "^2.3.3",
    "spectron": "^15.0.0",
    "typescript": "^4.4.0-beta"
  },
  "resolutions": {
    "node-gyp": "^5.0.2",
    "plist": "^3.0.4",
    "tar": "^4.4.18",
    "adal-node": "^0.2.3",
    "axios": "^0.21.4"
  }
}
